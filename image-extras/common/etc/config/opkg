config opkg 'defaults'
	option import '/etc/backup/installed_packages.txt'
	option restore 'init main'
	option rollback 'main'
	option upgr 'ai'
	option export 'ai'
	option proc '--force-depends'
	option reinstall '--force-reinstall'
	option newconf '/etc'

