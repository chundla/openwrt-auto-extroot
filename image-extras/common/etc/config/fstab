
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'

config mount 'rwm'
	option device '/dev/ubi0_1'
	option target '/rwm'

config mount 'extroot'
	option uuid '05d615b3-bef8-460c-9a23-52db8d09e000'
	option target '/overlay'

